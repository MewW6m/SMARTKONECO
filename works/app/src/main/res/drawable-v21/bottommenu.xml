    android:color="@color/konekocolor3">
    <item>
        <selector xmlns:android="http://schemas.android.com/apk/res/android">
            <item
                android:drawable="@drawable/cell7"
                android:state_checked="false" />
            <item
                android:drawable="@drawable/cell6"
                android:state_checked="true" />
            <item android:drawable="@drawable/cell7" />

        </selector>
    </item>
</ripple>
